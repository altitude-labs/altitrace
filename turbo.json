{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".next/**", "!.next/cache/**", "target/release/**", "docs/dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "outputs": ["target/debug/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "lint:check": {
      "dependsOn": ["^lint:check"]
    },
    "fmt:check": {
      "dependsOn": ["^fmt:check"]
    },
    "fmt": {
      "dependsOn": ["^fmt"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    }
  }
}